{
  "name": "qlik-sense-variable-editor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "0.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-observable": "^0.18.0",
    "rxjs": "^5.5.6",
    "rxq": "^1.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
